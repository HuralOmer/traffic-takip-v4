{
  "name": "@universal-tracking/service",
  "version": "1.0.0",
  "description": "Universal tracking service - Node.js",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup",
    "build:browser": "tsup --config tsup.config.browser.ts",
    "build:all": "pnpm build && pnpm build:browser",
    "build:tsc": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest unit",
    "test:integration": "jest integration",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist dist-browser"
  },
  "dependencies": {
    "@clickhouse/client": "^0.2.5",
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^11.1.1",
    "@universal-tracking/schemas": "workspace:*",
    "@universal-tracking/types": "workspace:*",
    "ajv": "^8.12.0",
    "fastify": "^4.24.3",
    "ioredis": "^5.3.2",
    "pg": "^8.11.3",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "prom-client": "^15.0.0",
    "redis": "^5.8.3",
    "ws": "^8.18.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.4",
    "tsup": "^8.0.1",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  }
}
